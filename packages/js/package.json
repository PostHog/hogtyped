{
  "name": "hogtyped",
  "version": "0.1.0",
  "description": "Code generator for type-safe PostHog wrappers with embedded schemas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "hogtyped": "./bin/hogtyped.js"
  },
  "files": [
    "dist",
    "bin",
    "schemas"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "posthog",
    "analytics",
    "typescript",
    "type-safe",
    "json-schema"
  ],
  "author": "PostHog",
  "license": "MIT",
  "peerDependencies": {
    "posthog-js": ">=1.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "posthog-js": "^1.96.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}